<div class="jumbotron jumbotron-fluid">
  <class class="container">
    <h1 class="header" id="pageType">Logs Summary</h1>
    <h2 class="subheader" id="project">{{this.projectName}}</h2>
    <h2 class="subheader" id="studentName">{{this.studentName}}</h2>
    <h2 class="subheader" id="time-log" style="font-size: large;"></h2>
    <h2 class="hidden" id="hidden-studentName">{{this.cntUser}}</h2>
    <h2 class="hidden" id="isUpdate">{{this.update}}</h2>
    {{#if this.logId}}
    <h2 class="subheader" id="logId">Record# {{this.logId}}</h2>
    <h2 class="hidden" id="hidden-logId">{{this.logId}}</h2>
    {{/if}}
    {{#unless this.cntUser}}
    {{> capacity}}
    {{/unless}}
</div>
{{>entry-key}}
{{>hidden-message}}
<div class="table-container">
  <table class="table table-striped">
    <thead>
      {{#unless this.studentName.length}}
        <tr id="form-row">
          <td colspan="12">
          <br>
            <div class="form-group">
              <label for="name">Name:</label>
                <select placeholder="Select Student" class="custom-select" id="inputGroupStudent">
                </select>
            </div>
            <div style="display: flex; justify-content: center; align-tiems: center;">
              <button type="submit" class="btn btn-success" id="Search">Search</button>
            </div>
          </td>
        </tr>
      {{/unless}}
      {{> cntUser}}
    </thead>
    <tbody>
    </tbody>
  </table>
</div>

{{#if this.update}}
<script src="assets/js/update_cntUser.js" type="text/javascript"></script>
{{else if this.studentName.length}}
<!-- Custom Script -->
<script src="assets/js/cntUser.js" type="text/javascript"></script>
{{else}}
<!-- Custom Script -->
<script src="assets/js/prj.js" type="text/javascript"></script>
{{/if}}