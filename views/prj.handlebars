<div class="jumbotron jumbotron-fluid">
  <class class="container">
    <h1 class="header" id="pageType">Project Summary</h1>
    <h2 class="subheader" id="project">{{this.projectName}}</h2>
    <h2 class="hidden" id="project-id">{{this.project_id}}</h2>
    <h2 class="subheader" id="time-log" style="font-size: large;"></h2>
    <h2 class="hidden" id="hidden-id">{{this.project_id}}</h2>
    {{#unless this.cntPrj}}
    {{> capacity}}
    {{/unless}}
</div>
{{> entry-key}}
{{>hidden-message}}
<button id="download2csv" style="display: block; margin-left: auto;">Download CSV</button>
<div class="table-container">
  <table class="table table-striped">
      <thead>
      <tr id="form-row">
        <td colspan="12">
          <br>
          <form class="index-form">
            <div class="form-group">
              <label for="category">Project:</label>
                <select placeholder="Enter the Category" class="custom-select" id="inputGroupProject">   
                </select>
            </div>
            <div style="display: flex; justify-content: center; align-tiems: center;">
              <button type="submit" class="btn btn-success" id="Search">Search</button>
            </div>
        </td>
      </tr>
      {{#if this.projectName.length}}
        {{> cntPrj}}
      {{else}}
        {{> cntPrj_list}}
      {{/if}}
    </thead>
    <tbody>
    </tbody>
  </table>
</div>


{{#if this.projectName.length}}
<!-- Custom Script -->
<script src="assets/js/prj.js" type="text/javascript"></script>
{{else}}
<!-- Custom Script -->
<script src="assets/js/prj_list.js" type="text/javascript"></script>
{{/if}}