<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="header">{{this.program}}</h1>
    <h2 class="subheader">Program Summary</h2>
    <h2 class="hidden" id="hiddenId">{{this.program}}</h2>
    <h2 class="subheader" id="#total-hours"></h2>
    <h2 class="subheader" id="#total-cost"></h2>
  </div>
</div>
<div class="table-container">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Log#</th>
        <th>Name</th>
        <th>Date</th>
        <th>Category</th>
        <th>Task</th>
        <th>Time Spent</th>
        <th>Program</th>
        <th>ECR</th>
        <th>Notes</th>
      </tr>
    </thead>
    <tbody>
    </tbody>
  </table>
</div>
<footer class="footer mt-auto py-3">
  <div class="container">
    {{> api-data}}
  </div>
</footer>
<!-- Custom Script -->
<script src="assets/js/rfb.js" type="text/javascript"></script>