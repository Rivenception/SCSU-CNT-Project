<div class="jumbotron jumbotron-fluid">
  <div class="container">
    <h1 class="header">Cost Analysis Tools</h1>
    <h2 class="subheader">{{this.employeeName}}</h2>
    <h2 class="hidden" id="hiddenId">{{this.user}}</h2>
  </div>
</div>
<div class="container" id="form-container">
  <form class="employeeEntry-form" colspan="12">
    <div class="form-group">
      <label for="dept">Program:</label>
      <input placeholder="Enter Program" list="program" id="programInput">
      <datalist id="program">
        <option value="Engineering">Engineering</option>
      </datalist>

      <label for="dept">ECR:</label>
      <input placeholder="Enter ECR" list="ecr" id="ecrInput">
      <datalist id="ecr">
      </datalist>

      <button type="submit" class="btn btn-success" id="analyze">Run Analysis</button>
  </form>
</div>
<!-- Custom Script -->
<script src="assets/js/analysis.js" type="text/javascript"></script>