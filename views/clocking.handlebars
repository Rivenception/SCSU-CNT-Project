<div class="jumbotron jumbotron-fluid">
  <class class="container">
    <h1 class="header">Time In/Out Summary</h1>
    <h2 class="subheader" id="studentName">{{this.studentName}}</h2>
    <h2 class="subheader" id="time-log" style="font-size: large;"></h2>
    <h2 class="hidden" id="hidden-studentName">{{this.cntUser}}</h2>
    <h2 class="hidden" id="hidden-student-id">{{this.student_id}}</h2>
    {{#unless this.cntUser}}
    {{> capacity}}
    {{/unless}}
</div>
{{>entry-key}}
<div class="table-container">
  <table class="table table-striped">
    <thead>
      {{> clocking}}
    </thead>
    <tbody>
    </tbody>
  </table>
</div>

{{#if this.studentName.length}}
<!-- Custom Script -->
<script src="assets/js/clocking.js" type="text/javascript"></script>
{{else}}
<!-- Custom Script -->
<script src="assets/js/clocking_list.js" type="text/javascript"></script>
{{/if}}